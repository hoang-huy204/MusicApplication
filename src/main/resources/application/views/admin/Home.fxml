<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="borderPaneRoot" prefHeight="635.0" prefWidth="938.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.AdminController">
    <top>
        <AnchorPane prefHeight="54.0" prefWidth="954.0" BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="440.0" layoutY="11.0" text="DashBoard Admin">
                    <font>
                        <Font name="System Bold" size="25.0" />
                    </font>
                </Label>
                <ImageView fitHeight="46.0" fitWidth="66.0" layoutX="891.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true" styleClass="border_wrap">
                    <image>
                        <Image url="@../../image/ava.jpg" />
                    </image>
                </ImageView>
                <ImageView fitHeight="53.0" fitWidth="53.0" layoutX="363.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../image/dashicon.jpg" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
    </top>
    <left>
        <AnchorPane prefHeight="625.0" prefWidth="234.0" BorderPane.alignment="CENTER">
            <children>
                <ImageView fitHeight="46.0" fitWidth="55.0" layoutX="17.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true" styleClass="border_wrap">
                    <image>
                        <Image url="@../../image/dashicon.jpg" />
                    </image>
                </ImageView>
                <Label fx:id="dashBroardSwitch" layoutX="85.0" layoutY="39.0" onMousePressed="#switchMenu" text="DashBoard">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Label>
                <Label fx:id="playlistSwitch" layoutX="85.0" layoutY="110.0" onMousePressed="#switchMenu" text="PlayList">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Label>
                <Label fx:id="trackSwitch" layoutX="85.0" layoutY="181.0" onMousePressed="#switchMenu" text="Track">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Label>
                <Label fx:id="userSwitch" layoutX="83.0" layoutY="255.0" onMousePressed="#switchMenu" text="User">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Label>
                <ImageView fitHeight="46.0" fitWidth="55.0" layoutX="17.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../image/playlist.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="46.0" fitWidth="55.0" layoutX="15.0" layoutY="173.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../image/track.jpg" />
                    </image>
                </ImageView>
                <ImageView fitHeight="46.0" fitWidth="55.0" layoutX="17.0" layoutY="244.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../image/user.png" />
                    </image>
                </ImageView>
            <Button layoutX="21.0" layoutY="532.0" mnemonicParsing="false" onAction="#handleLogout" prefHeight="26.0" prefWidth="188.0" style="-fx-background-color: red;" text="Logout" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label fx:id="genreSwitch" layoutX="83.0" layoutY="329.0" onMousePressed="#switchMenu" text="Genre">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <ImageView fitHeight="53.0" fitWidth="57.0" layoutX="12.0" layoutY="316.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../image/genre.png" />
               </image>
            </ImageView>
            <Label fx:id="packageSwitch" layoutX="82.0" layoutY="398.0" onMousePressed="#switchMenu" text="Package">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <ImageView fitHeight="53.0" fitWidth="57.0" layoutX="11.0" layoutY="385.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../image/package.png" />
               </image>
            </ImageView>
            </children>
        </AnchorPane>
    </left>
</BorderPane>
